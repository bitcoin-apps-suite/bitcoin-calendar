{
  "workbench.colorTheme": "Default Dark+",
  "workbench.colorCustomizations": {
    "statusBar.noFolderBackground": "#FFCC00",
    "activityBar.activeBorder": "#FFD700",
    "sideBar.background": "#252526",
    "sideBar.foreground": "#CCCCCC",
    "sideBarSectionHeader.background": "#3C3C3C",
    "sideBarSectionHeader.foreground": "#FFFFFF",
    "panel.background": "#1E1E1E",
    "panelTitle.activeBorder": "#FFD700",
    "tab.activeBackground": "#1E1E1E",
    "tab.activeForeground": "#FFFFFF",
    "tab.inactiveBackground": "#2D2D30",
    "tab.inactiveForeground": "#969696",
    "editor.background": "#1E1E1E",
    "editor.foreground": "#D4D4D4",
    "editor.selectionBackground": "#264F78",
    "editor.lineHighlightBackground": "#2A2D2E",
    "editorGroupHeader.tabsBackground": "#252526",
    "breadcrumb.background": "#FFD700",
    "breadcrumb.foreground": "#000000",
    "button.background": "#FFD700",
    "button.foreground": "#000000",
    "button.hoverBackground": "#FFA500",
    "input.background": "#3C3C3C",
    "input.border": "#FFD700",
    "focusBorder": "#FFD700",
    "list.activeSelectionBackground": "#094771",
    "list.activeSelectionForeground": "#FFFFFF",
    "list.hoverBackground": "#2A2D2E",
    "scrollbarSlider.background": "#79797966",
    "scrollbarSlider.hoverBackground": "#646464B3",
    "terminal.background": "#1E1E1E",
    "terminal.foreground": "#D4D4D4",
    "terminalCursor.background": "#FFD700",
    "terminalCursor.foreground": "#FFD700"
  },
  "editor.fontSize": 14,
  "editor.fontFamily": "'SF Mono', 'Monaco', 'Fira Code', 'Consolas', monospace",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.wordWrap": "on",
  "editor.minimap.enabled": true,
  "editor.rulers": [80, 120],
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 1000,
  "typescript.preferences.quoteStyle": "single",
  "javascript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "prettier.trailingComma": "es5",
  "emmet.includeLanguages": {
    "typescript": "html",
    "typescriptreact": "html"
  }
}